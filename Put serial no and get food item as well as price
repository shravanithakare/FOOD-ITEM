#include <stdio.h>
int main()
{
    printf("Select the random numer from 1 to 5:");
    int ch;
    scanf("%d", &ch);
    switch (ch)
    {
    case 1:
        printf("Food item-Pizza\n");
        printf("Prize-239");
        break;
    case 2:
        printf("Food item-Burger\n");
        printf("Prize-129");
        break;
    case 3:
        printf("Food item-Pasta\n");
        printf("Prize-179");
        break;
    case 4:
        printf("Food item-French Fries\n");
        printf("Prize-99");
        break;
    case 5:
        printf("Food item-Sandwich\n");
        printf("Prize-149");
        break;
    default:
        printf("Not available");
    }
    return 0;
}
